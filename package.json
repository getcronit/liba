{
  "name": "jaen-template",
  "description": "Template for Jaen projects",
  "version": "1.0.0",
  "private": true,
  "author": "Nico Schett",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "NODE_OPTIONS=--max-old-space-size=8192 gatsby develop -H 0.0.0.0",
    "start": "yarn develop",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@atsnek/jaen": "^1.0.0-rc.22",
    "gatsby": "^5.11.0",
    "gatsby-plugin-jaen": "^1.0.0-rc.34",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "typescript": "^5.1.6"
  }
}
